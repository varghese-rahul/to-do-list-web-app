{
	"manifest_version": 2,
	"name": "To Do List",
	"version": "1.0",
	"description": "A convenient and simple to do list",
	"permissions": [
   	 "activeTab",
    	"storage"
	],
	"icons": {
    	"128" : "tdl.png"
	},
	"browser_action": {
    	"default_icon": "tdl.png",
    	"default_popup": "index.html",
    	"default_title": "To Do List"
	}
}